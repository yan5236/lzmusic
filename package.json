{
  "name": "lzmusic",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "build:react": "vite build",
    "build:electron": "tsc -p tsconfig.electron.json && tsc -p tsconfig.preload.json",
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:dist": "npm run build:react && npm run build:electron && electron .",
    "dev:react": "vite dev",
    "dev:electron": "chcp 65001 && npm run build:electron && cross-env NODE_ENV=development electron .",
    "build": "npm run build:react && npm run build:electron",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:linux": "npm run build && electron-builder --linux",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@tailwindcss/vite": "^4.1.18",
    "better-sqlite3": "^12.4.5",
    "electron-updater": "^6.6.2",
    "lucide-react": "^0.554.0",
    "music-metadata": "^11.10.3",
    "NeteaseCloudMusicApi": "^4.28.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.3",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.1.2",
    "cross-env": "^10.1.0",
    "electron": "^38.2.2",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
